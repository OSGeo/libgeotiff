\pard\plain \s253\li360\ri-360\sb120\sa120\keepn \b\f20\fs28 6.1 Tag ID Summary\par 
\pard\plain \s10\ri-360\keep\tx880\tx1740\tx2620\tx3480\tx4320\tx5220\tx6060\tx6940\tx7780 \f22\fs20 \par 
